{
  "manifest_version": 2,

  "name": "Douban Read Helper",
  "description": "Douban Read Helper",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Douban Read Helper"
  },
  "content_scripts": [
    {
      "matches": ["http://book.douban.com/subject/*"],
      "js": ["jquery-1.9.1.min.js", "modifier.js"]
    }
  ]
}
